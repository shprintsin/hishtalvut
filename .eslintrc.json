{
  "extends": [
    "next",
    "turbo",
    "next/core-web-vitals",
    "prettier",
    "eslint:recommended",
     "next"
  ],
  "plugins": ["react-hooks", "prettier"],
  "rules": {
    "no-unused-vars": "off",
    "prettier/prettier": "warn",
    "react-hooks/rules-of-hooks": "warn",
    "react-hooks/exhaustive-deps": "warn"
    
  }
}
